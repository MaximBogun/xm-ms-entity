{
    "_source": {
        "includes": [
            "id",
            "key",
            "data.*",
            "typeKey",
            "name",
            "startDate",
            "updateDate",
            "version"
        ],
        "excludes": [
            "data.notSaveField"
        ]
    },
    "properties": {
        "id": {
            "type": "long"
        },
        "key": {
            "type": "string",
            "index": "not_analyzed"
        },
        "typeKey": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "startDate": {
            "type": "string"
        },
        "updateDate": {
            "type": "string"
        },
        "version": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "targetField": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "notSaveField": {
                    "type": "string"
                }
            }
        }
    }
}
